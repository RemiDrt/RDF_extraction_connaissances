            owl:someValuesFrom xsd:string ] .

ace:field_level a owl:ObjectProperty ;
    rdfs:domain ace:Field ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:string ] .

ace:field_name a owl:ObjectProperty ;
    rdfs:domain ace:Field ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:string ] .

