ace:paper_rank a owl:ObjectProperty ;
    rdfs:domain ace:Paper ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:int ] .

ace:paper_sci_citation a owl:ObjectProperty ;
    rdfs:domain ace:Paper ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:int ] .

ace:paper_title a owl:ObjectProperty ;
    rdfs:domain ace:Paper ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:string ] .

ace:work_in a owl:ObjectProperty ;
    rdfs:domain ace:Institute ;
    rdfs:range ace:Author .

ace:Journal a owl:Class ;
    rdfs:subClassOf ace:Venue .

ace:Affiliation a owl:Class .

xsd:date a rdfs:Datatype .

ace:Conference a owl:Class ;
    rdfs:subClassOf ace:Venue .

ace:Venue a owl:Class .

ace:Institute a owl:Class .

ace:Author a owl:Class .

ace:Field a owl:Class .

ace:Paper a owl:Class .

