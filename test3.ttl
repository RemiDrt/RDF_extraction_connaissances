@prefix ace: <http://www.semanticweb.org/acemap#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.semanticweb.org/acemap> a owl:Ontology .

ace:affiliation_name a owl:ObjectProperty ;
    rdfs:domain ace:Affiliation ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:string ] .

ace:author_citation_count a owl:ObjectProperty ;
    rdfs:domain ace:Author ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:int ] .

ace:author_is_in_field a owl:ObjectProperty ;
    rdfs:domain ace:Author ;
    rdfs:range ace:Field .

ace:author_name a owl:ObjectProperty ;
    rdfs:domain ace:Author ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:string ] .

ace:author_number_of_paper a owl:ObjectProperty ;
    rdfs:domain ace:Author ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:int ] .

ace:author_sci_citation a owl:ObjectProperty ;
    rdfs:domain ace:Author ;
    rdfs:range [ a owl:Restriction ;
            owl:onProperty owl:topDataProperty ;
            owl:someValuesFrom xsd:int ] .

ace:conference_full_name a owl:ObjectProperty ;
